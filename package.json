{
  "name": "mink",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "vite dev --host",
    "build": "vite build",
    "preview": "vite preview",
    "start": "pnpm run start:prisma && pnpm run start:node",
