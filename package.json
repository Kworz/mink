    "@threlte/extras": "8.10.3",
    "@types/dxf": "^4.6.7",
    "@types/qrcode": "^1.5.5",
    "@types/three": "^0.162.0",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "autoprefixer": "^10.4.18",
    "dxf": "^5.1.0",
    "eslint": "^8.57.0",
    "eslint-plugin-svelte": "^2.35.1",
